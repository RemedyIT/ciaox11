        taox11_cosnaming_stub, ciaox11_logger, dancex11_stub, dancex11_state {
  dynamicflags += CIAOX11_DEPLOYMENT_INTERCEPTORS_BUILD_DLL

  Source_Files {
    deployment_interceptors.cpp
    name_utilities.cpp
