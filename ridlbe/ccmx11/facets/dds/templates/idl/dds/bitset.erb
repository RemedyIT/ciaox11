
// generated from <%= ridl_template_path %>
/// @copydoc <%= doc_scoped_name %>
% annotations.each do |_a|
<%= _a.idl_stringified %>
% end
bitset <%= unescaped_name %><% unless base.nil? %> : <%= base.unescaped_name %><% end %> {
% bitfields.each do | e |
  /// @copydoc <%= e.doc_scoped_name %>
% e.annotations.each do |_a|
  <%= _a.idl_stringified %>
% end
  bitfield<<%= e.bits %>, <%= e.idltype_name %>><% unless e.name.empty? %> <%= e.name %><% end %>;
% end
};

