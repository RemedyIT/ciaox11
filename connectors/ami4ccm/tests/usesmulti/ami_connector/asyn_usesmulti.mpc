// The local executor mapping of the A.idl is only used in the AMI4CCM connector
// implementation. Since the AE.idl will not be generated for the pattern specific
// stub it should be created here.
project(*ami4ccm_lem_gen) : ciaox11_test_base_lem_idl, ciaox11_ami4ccm {
  custom_only = 1

  after += asyn_usesmulti_ami4ccm_aidl_gen

  idlflags += -Wb,base_export_macro=USESMULTI \
              -Wb,base_export_include=usesmulti

  IDL_Files {
    gendir=generated_code
    ../base/generated_code/usesmultiA.idl
  }
}

project(*ami4ccm_idl_gen) : ciaox11_test_ami4ccm_conn_all_idl {
  custom_only = 1

  after += asyn_usesmulti_lem_gen *ami4ccm_lem_gen

  idlflags += -Wb,base_export_macro=USESMULTI \
              -Wb,base_export_include=usesmulti

  IDL_Files {
    gendir=generated_code
    ../base/usesmulti.idl
  }
}

project(*ami4ccm_conn) : ciaox11_test_base_stub, ciaox11_executor, ciaox11_ami4ccm_impl {
  after += asyn_usesmulti_ami4ccm_stub \
           asyn_usesmulti_stub \
           asyn_usesmulti_lem_stub \
           *ami4ccm_idl_gen
  libs  += asyn_usesmulti_ami4ccm_stub \
           asyn_usesmulti_stub \
           asyn_usesmulti_lem_stub

  dynamicflags += USESMULTI_AMIC_BUILD_DLL \
                  USESMULTI_LEM_STUB_BUILD_DLL \
                  USESMULTI_LEM_STUB_AMI_CONN_BUILD_DLL \
                  USESMULTI_AMI4CCM_CONN_BUILD_DLL \
                  USESMULTI_AMI4CCM_SVNT_BUILD_DLL

  Source_Files {
    generated_code/usesmultiAmiC.cpp
    generated_code/usesmultiAEC.cpp
    generated_code/usesmultiA_connEC.cpp
    generated_code/usesmultiA_conn_exec.cpp
    generated_code/usesmultiA_conn_svnt.cpp
  }

  Header_Files {
    generated_code/usesmultiAmiC.h
    generated_code/usesmultiAEC.h
    generated_code/usesmultiA_connEC.h
    generated_code/usesmultiA_conn_exec.h
    generated_code/usesmultiA_conn_svnt.h

    generated_code/usesmulti_amic_export.h
    generated_code/usesmulti_lem_stub_export.h
    generated_code/usesmulti_lem_stub_con_export.h
    generated_code/usesmulti_ami4ccm_conn_export.h
    generated_code/usesmulti_ami4ccm_svnt_export.h
  }
}

