% visit_component_init_connect
  // generated from <%= ridl_template_path %>
% if all_incl_receptacle_port?
%   if all_incl_multiple_port?
  return this->component_context_->set_connection (name, std::move(connection), conn_name, config);
%   else
  X11_UNUSED_ARG(conn_name);
  return this->component_context_->set_connection (name, std::move(connection), config);
%   end
% else
  X11_UNUSED_ARG(name);
  X11_UNUSED_ARG(connection);
  X11_UNUSED_ARG(conn_name);
  X11_UNUSED_ARG(config);
  return false;
% end
