// Compile section: exec
libs += <%= link_libs.join(" ") %>
%if !link_lit_libs.empty?
lit_libs += <%= link_lit_libs.join(" ") %>
%end
libout    = <%= lib_out %>
libpaths += <%= lib_paths.join(" ") %>
includes += <%= includes.join(" ") %>

sharedname    = <%= shared_name %>
dynamicflags += <%= dynamic_names.join(" \\\n                ") %>

Source_Files {
% sources.each do |s|
  <%= s %>
% end
}

Header_Files {
% headers.each do |h|
  <%= h %>
% end
}
