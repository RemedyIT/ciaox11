  <instance xmi:id="Hello.ComponentImplementations.HelloImplementation.Conn_Test_ConnR">
    <name>Hello.ComponentImplementations.HelloImplementation.Conn_Test_ConnR</name>
    <node>ReceiverNode</node>
    <source/>
    <implementation xmi:idref="Conn_Test_ConnRImplementation"/>
    <configProperty>
      <name>conn_attrib</name>
      <value>
        <type>
          <kind>tk_short</kind>
        </type>
        <value>
          <short>4321</short>
        </value>
      </value>
    </configProperty>
    <configProperty>
      <name>consumer_port_attrib</name>
      <value>
        <type>
          <kind>tk_short</kind>
        </type>
        <value>
          <short>4321</short>
        </value>
      </value>
    </configProperty>
  </instance>

  <!--connection between Receiver and ConnectorR-->
  <connection>
    <name>receiver_conn_connection</name>
    <internalEndpoint>
      <portName>consumer_foo_prov</portName>
      <provider>false</provider>
      <kind>SimplexReceptacle</kind>
      <instance xmi:idref="Hello.ComponentImplementations.HelloImplementation.Conn_Test_ConnR" />
    </internalEndpoint>
    <internalEndpoint>
      <portName>rec_listen_port_foo_prov</portName>
      <provider>true</provider>
      <kind>Facet</kind>
      <instance xmi:idref="Hello.ComponentImplementations.HelloImplementation.Hello.Receiver" />
    </internalEndpoint>
  </connection>

  <!--connection between Sender and Connectors-->
  <connection>
    <name>sender_conn_connection</name>
    <internalEndpoint>
      <portName>supplier_foo_uses</portName>
      <provider>true</provider>
      <kind>Facet</kind>
      <instance xmi:idref="Hello.ComponentImplementations.HelloImplementation.Conn_Test_ConnS" />
    </internalEndpoint>
    <internalEndpoint>
      <portName>sender_write_port_foo_uses</portName>
      <provider>false</provider>
      <kind>SimplexReceptacle</kind>
      <instance xmi:idref="Hello.ComponentImplementations.HelloImplementation.Hello.Sender" />
    </internalEndpoint>
  </connection>

  <artifact xmi:id="Hello_Receiver_ExecArtifact">
