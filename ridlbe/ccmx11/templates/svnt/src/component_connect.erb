% visit_init_connect
  // generated from <%= ridl_template_path %>
% if all_incl_receptacle_port?
%   if all_incl_multiple_port?
  return this->component_context_->set_connection (name,
                                                   std::move(connection),
                                                   conn_name);
%   else
  X11_UNUSED_ARG(conn_name);
  return this->component_context_->set_connection (name, std::move(connection));
%   end
% else
  X11_UNUSED_ARG(name);
  X11_UNUSED_ARG(connection);
  X11_UNUSED_ARG(conn_name);
  return false;
% end
