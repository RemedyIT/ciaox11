// The local executor mapping of the A.idl is only used in the AMI4CCM connector
// implementation. Since the AE.idl will not be generated for the pattern specific
// stub it should be created here.
project(*ami4ccm_lem_gen) : ciaox11_test_base_lem_idl, ciaox11_ami4ccm {
  custom_only = 1

  after += return_args_t_ami4ccm_aidl_gen

  idlflags += -Wb,base_export_macro=RETURN_ARGS_T \
              -Wb,base_export_include=return_args_t

  IDL_Files {
    gendir=generated_code
    ../base/generated_code/return_args_tA.idl
  }
}

project(*ami4ccm_idl_gen) : ciaox11_test_ami4ccm_conn_all_idl {
  custom_only = 1

  after += return_args_t_lem_gen *ami4ccm_lem_gen

  idlflags += -Wb,base_export_macro=RETURN_ARGS_T \
              -Wb,base_export_include=return_args_t

  IDL_Files {
    gendir=generated_code
    ../base/return_args_t.idl
  }
}

project(*ami4ccm_conn) : ciaox11_test_base_stub, ciaox11_executor, ciaox11_ami4ccm_impl {
  after += return_args_t_ami4ccm_stub return_args_t_stub return_args_t_lem_stub *ami4ccm_idl_gen
  libs  += return_args_t_ami4ccm_stub return_args_t_stub return_args_t_lem_stub

  dynamicflags += RETURN_ARGS_T_AMIC_BUILD_DLL \
                  RETURN_ARGS_T_LEM_STUB_BUILD_DLL \
                  RETURN_ARGS_T_LEM_STUB_AMI_CONN_BUILD_DLL \
                  RETURN_ARGS_T_AMI4CCM_CONN_BUILD_DLL \
                  RETURN_ARGS_T_AMI4CCM_SVNT_BUILD_DLL

  Source_Files {
    generated_code/return_args_tAmiC.cpp
    generated_code/return_args_tAEC.cpp
    generated_code/return_args_tA_connEC.cpp
    generated_code/return_args_tA_conn_exec.cpp
    generated_code/return_args_tA_conn_svnt.cpp
  }

  Header_Files {
    generated_code/return_args_tAmiC.h
    generated_code/return_args_tAEC.h
    generated_code/return_args_tA_connEC.h
    generated_code/return_args_tA_conn_exec.h
    generated_code/return_args_tA_conn_svnt.h

    generated_code/return_args_t_amic_export.h
    generated_code/return_args_t_lem_stub_export.h
    generated_code/return_args_t_lem_stub_ami_conn_export.h
    generated_code/return_args_t_ami4ccm_conn_export.h
    generated_code/return_args_t_ami4ccm_svnt_export.h
  }
}

