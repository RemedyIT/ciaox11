      <value>
        <type>
          <kind>tk_struct</kind>
          <struct>
            <name>QueryFilter</name>
            <typeId>IDL:CCM_DDS/QueryFilter:1.0</typeId>
            <member>
              <name>expression</name>
              <type>
                <kind>tk_string</kind>
              </type>
            </member>
            <member>
              <name>parameters</name>
              <type>
                <kind>tk_sequence</kind>
                <sequence>
                  <elementType>
                    <kind>tk_string</kind>
                  </elementType>
                </sequence>
              </type>
            </member>
          </struct>
        </type>
        <value>
          <member>
            <name>expression</name>
            <value>
              <string>( (iteration &lt; %0) OR (iteration &gt; %1) ) OR (key = 'KEY_6')</string>
            </value>
          </member>
          <member>
            <name>parameters</name>
            <value>
              <element>
                <string>6</string>
              </element>
              <element>
                <string>9</string>
